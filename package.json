{
  "name": "blog",
  "version": "0.0.1",
  "scripts": {
    "dev": "svelte-kit dev",
    "package": "svelte-kit package",
    "preview": "svelte-kit preview",
    "check": "svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
    "deploy": "npx gh-pages -d build -t true",
    "build": "run-script-os",
    "build:win32": "(if exist build rmdir /Q /S build) && svelte-kit build && wsl touch build/.nojekyll",
    "build:darwin:linux": "rm -rf build && svelte-kit build && touch build/.nojekyll",
    "publish": "npm run build && npm run deploy"
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "next",
    "@sveltejs/adapter-static": "^1.0.0-next.24",
    "@sveltejs/kit": "next",
    "@typescript-eslint/eslint-plugin": "^4.31.1",
    "@typescript-eslint/parser": "^4.31.1",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-svelte3": "^3.2.1",
    "gh-pages": "^3.2.3",
    "prettier": "^2.4.1",
    "prettier-plugin-svelte": "^2.4.0",
    "run-script-os": "^1.1.6",
    "svelte": "^3.44.0",
    "svelte-check": "^2.2.6",
    "svelte-preprocess": "^4.9.4",
    "tslib": "^2.3.1",
    "typescript": "^4.4.3",
    "vite": "^2.7.13"
  },
  "type": "module",
  "dependencies": {
    "@mapbox/rehype-prism": "^0.8.0",
    "front-matter": "^4.0.2",
    "glob": "^7.2.0",
    "rehype": "^12.0.0",
    "remark": "^14.0.2",
    "remark-html": "^15.0.1",
    "remark-rehype": "^10.1.0",
    "svelte-awesome": "^2.4.2"
  }
}
